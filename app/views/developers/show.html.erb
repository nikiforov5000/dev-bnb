<div class="container margin-top-negative">
	<div class="wrapper">
		<div class="profile">
			<%= image_tag "https://raw.githubusercontent.com/lewagon/fullstack-images/master/uikit/logo.png", class: "thumbnail" %>
			<h3 class="name"><%= @developer.name %></h3>
			<p class="title">Profile</p>
			<p class="title">$<%= @developer.daily_rate %></p>
			<p class="description"><%= @developer.bio %></p>
			<button type="button" class="btn"><%= link_to "Hire developer", new_developer_booking_path(@developer)  %> </button>
		</div>
	</div>
</div>

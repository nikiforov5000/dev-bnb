<div class="container">
	<div class="wrapper">
		<div class="profile"> 
			<%#= image_tag "https://images.unsplash.com/photo-1499557354967-2b2d8910bcca?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=1270&q=80", class: "thumbnail" %>
			<% if @developer.photo.attached? %>
				<%= cl_image_tag @developer.photo.key, class: "thumbnail" %>	
			<% end %>
			<h4 class="name"><%= @developer.name %></h4>
			<p class="title">Profile</p>
			<p class="title">$<%= @developer.daily_rate %></p>
			<p class="description"><%= @developer.bio %></p>
			<button type="button" class="btn"><%= link_to "Hire", new_developer_booking_path(@developer)  %> </button>
		</div>
	</div>
</div>
